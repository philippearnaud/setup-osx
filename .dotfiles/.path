# Add Catchmail to PATH
PATH=/Users/mtomas/.rvm/gems/ruby-2.1.3/bin:$PATH

# Add Python 3.4 to PATH
PATH=/Library/Frameworks/Python.framework/Versions/3.4/bin:$PATH

# Add GDAL Framework to PATH
PATH=/Library/Frameworks/GDAL.framework/Programs:$PATH

# Add Android SDK Platform Tools to PATH
PATH=/Applications/Android\ SDK/sdk/platform-tools:$PATH

# Add Homebrew PHP 5.6 bin to PATH
PATH="$(brew --prefix homebrew/php/php56)/bin":$PATH

# Add user binaries to PATH
PATH=$HOME/bin:$PATH

# Add homebrew & others binaries to PATH
PATH=/usr/local/bin:/usr/local/sbin:$PATH

# Add composer global binaries to PATH 
PATH=$HOME/.composer/vendor/bin:$PATH

# Add RVM to PATH for scripting
PATH=$PATH:$HOME/.rvm/bin

# finally export PATH
export PATH

# GO lang path
export GOPATH=$HOME/go

# Android SDK Home
export ANDROID_HOME=/usr/local/opt/android-sdk

# XML Catalog path
export XML_CATALOG_FILES=/usr/local/etc/xml/catalog

